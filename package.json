{
  "name": "svelte-boilerplate",
  "author": "Konrad <g.konrad@outlook.com>",
  "license": "MIT",
  "version": "0.0.1",
  "bugs": "https://github.com/k-onrad/svelte-boilerplate/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:k-onrad/svelte-boilerplate.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:legacy": "cross-env LEGACY_BUILD=true rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv static --port 3000",
    "cypress:open": "cypress open",
    "lint": "eslint -c .eslintrc.json --ext .svelte,.js src",
    "format": "eslint -c .eslintrc.json --fix --ext .svelte,.js --fix src",
    "check": "es-check es5 './static/*.js'"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "core-js": "^3.6.5",
    "cross-env": "^5.2.1",
    "cypress": "^4.4.0",
    "es-check": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^4.13.1",
    "postcss": "^7.0.27",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-postcss": "^2.6.4",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sirv": "^0.4.2",
    "sirv-cli": "^0.4.5",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^2.16.0",
    "svelte-spa-router": "^2.1.0"
  },
  "dependencies": {}
}
